{
    "version": 2,
    "buildCommand": "vite build",
    "installCommand": "npm install",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/auth/(.*)",
            "destination": "/api-server.js"
        },
        {
            "source": "/agents(.*)",
            "destination": "/api-server.js"
        },
        {
            "source": "/wallets(.*)",
            "destination": "/api-server.js"
        },
        {
            "source": "/public/(.*)",
            "destination": "/api-server.js"
        },
        {
            "source": "/health",
            "destination": "/api-server.js"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}